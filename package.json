{
  "name": "vite-solid-hackernews",
  "description": "A Solid Implementation of the Hacker News Example App with Vite",
  "scripts": {
    "dev": "jiti server",
    "serve": "wrangler dev",
    "build": "pnpm build:client && pnpm build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "rollup -c vite-worker.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryansolid/solid-hackernews.git"
  },
  "keywords": [
    "Solid",
    "Hacker News",
    "example",
    "vite"
  ],
  "author": "Ryan Carniato",
  "contributors": [
    {
      "name": "Alexandre Mouton-Brady"
    }
  ],
  "license": "MIT",
  "main": "./dist/worker/entry-worker.js",
  "bugs": {
    "url": "https://github.com/amoutonbrady/solid-hackernews/issues"
  },
  "homepage": "https://github.com/amoutonbrady/solid-hackernews/",
  "dependencies": {
    "serve-static": "^1.14.1",
    "solid-app-router": "0.0.32",
    "solid-js": "0.24.14"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-typescript": "^7.13.0",
    "@cloudflare/kv-asset-handler": "^0.1.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.31",
    "express": "^4.17.1",
    "jiti": "^1.6.0",
    "node-fetch": "^2.6.1",
    "rollup-plugin-ignore-import": "^1.3.2",
    "rollup-route-manifest": "^1.0.0",
    "rollup-pluginutils": "^2.8.2",
    "vite": "^2.0.5",
    "vite-plugin-solid": "^1.3.0"
  }
}
